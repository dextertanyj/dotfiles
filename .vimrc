set nocompatible

set wrap
set showmatch

set autoindent
set smarttab
set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4

filetype plugin indent on

set showmode
set title

set ruler
set number
set runtimepath=~/dotfiles/custom/vim,$VIMRUNTIME
set packpath=~/dotfiles/custom/vim,$VIMRUNTIME

syntax on

colorscheme codedark 
